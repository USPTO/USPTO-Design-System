  "bugs": {
    "url": "https://github.com/USPTO/USPTO-DS-Theme/issues"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "clipboard": "^2.0.8",
    "glob": "^7.1.7",
    "nunjucks": "^3.2.3",
    "nunjucks-extension-code-highlight": "*",
    "prismjs": "^1.23.0",
    "sass": "^1.34.1",
    "usptostrap": "^2.2.3"
