{
  "name": "USPTO-DS",
  "version": "2.0.4",
  "description": "USPTO Design System. Contains SASS, fonts, icons for building USPTO applications.",
  "directories": {
    "dist": "dist"
  },
  "scripts": {
    "build": "node ./scripts/build",
    "dev": "node ./scripts/dev",
    "serve": "node ./scripts/server",
    "publish-site": "node ./scripts/publish-site",
    "pretty-scss": "prettier ./src/assets/scss/**/*.scss --write",
    "pretty-html": "prettier ./src/pages/**/*.html --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/USPTO/USPTO-DS-Theme.git"
  },
  "keywords": [
    "USPTO",
    "ip",
    "dpl",
    "sass",
    "fonts",
    "icons"
  ],
  "author": "USPTO",
  "contributors": [
    "Scott Merker <scott.merker@uspto.gov>",
    "Zoin Amir <zoin.amir@uspto.gov>",
    "Menka Kayan <menka.kayan@uspto.gov>"
  ],
  "private": true,
  "bugs": {
    "url": "https://github.com/USPTO/USPTO-DS-Theme/issues"
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "clipboard": "^2.0.6",
    "glob": "^7.1.6",
    "nunjucks": "^3.2.2",
    "nunjucks-extension-code-highlight": "*",
    "prismjs": "^1.22.0",
    "sass": "^1.27.1",
    "usptostrap": "^2.0.5"
  },
  "devDependencies": {
    "browser-sync": "^2.26.13",
    "fs-extra": "^9.0.1",
    "prettier": "^2.1.2"
  }
}
