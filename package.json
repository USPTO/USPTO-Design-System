  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "clipboard": "^2.0.6",
    "glob": "^7.1.6",
    "nunjucks": "^3.2.2",
    "nunjucks-extension-code-highlight": "*",
    "prismjs": "^1.21.0",
    "sass": "^1.26.10",
    "usptostrap": "^2.0.1"
  },
  "devDependencies": {
    "browser-sync": "^2.26.12",
    "fs-extra": "^9.0.1",
    "prettier": "^2.0.5"
