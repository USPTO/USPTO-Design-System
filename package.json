{
  "name": "USPTO-DPL",
  "version": "0.0.2",
  "description": "USPTO Design Pattern Library. Contains SASS, fonts, icons for building a USPTO styled application.",
  "directories": {
    "dist": "dist"
  },
  "scripts": {
    "build": "node ./scripts/build",
    "dev": "node ./scripts/dev",
    "serve": "node ./scripts/server",
    "pretty-scss": "prettier ./src/assets/scss/**/*.scss --write"
  },
  "repository": {
